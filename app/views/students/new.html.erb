

<h1>Student Form</h1>


<%= form_tag students_path do %>
    <label for="first_name"></label>
    <input type="text" name="student[first_name]" id="first_name">

    <label for="last_name"></label>
    <input type="text" name="student[last_name]" id="last_name">

    <%= submit_tag "Submit Student" %>
<% end %>